<h3>Registration Here </h3>

<%= form_for @registration, :as => :user, :url => user_create_path , :method => "post", :html => {:multipart => true} do |f| %>

<p>
<div class="field">
  <label>First Name:</label>
  <%= f.text_field :first_name %>
</div>
</p>

<p>
<div class="field">
  <label>Last Name:</label>
  <%= f.text_field :last_name %>
</div>
</p>


<p>
<div class="field">
  <label>Gender:</label>
  <%= f.text_field :gender %>
</div>
</p>


<p>
<div class="field">
  <label>City:</label>
  <%= f.text_field :first_name %>
</div>
</p>


<p>
<div class="field">
  <label>Country</label>
  <%= f.text_field :country %>
</div>
</p>


<p>
<div class="field">
  <label>Country</label>
  <%= f.text_field :state %>
</div>
</p>


<p>
<div class="field">
  <label>Address:</label>
  <%= f.text_field :address %>
</div>
</p>


<p>
<div class="field">
  <label>Zip-code: </label>
  <%= f.text_field :zipcode %>
</div>
</p>


<p>
<div class="field">
  <label>Profile  Picture</label>
  <%= f.text_field :profile_picture %>
</div>
</p>

<p>
<div class="field">
  <%= f.submit "Register" %>
</div>
</p>

<% end %>